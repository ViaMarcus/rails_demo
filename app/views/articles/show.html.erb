<% if user_signed_in? && @article.author == current_user.username %>
<nav>
    <a> <%= button_to "Edit Article", edit_article_path( @article ), method: :get, class: "btn neutral" %> </a>
</nav>
<% end %>
<p> <%= @article.title %> </p>
<p>Author: <i> <%= @article.author || "Anonymous" %> </i> </p>
<p> <%= @article.content %> </p>