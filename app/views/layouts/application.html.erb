<!DOCTYPE html>
<html>
  <head>
    <title>RailsDemo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <%= render 'layouts/nav' %>
  




    <% flash.each do |key, value| %>
    <%= content_tag(:a, value, class: key)%>
    <% end %>
    <div id="content">
      <%= yield %>
    </div>
  </body>
</html>
